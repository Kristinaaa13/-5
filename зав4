# 4) K-й найменший елемент у BST (ітеративний inorder)
def kthSmallest(root: TreeNode, k: int) -> int:
    st = []
    cur = root
    while cur or st:
        while cur:
            st.append(cur); cur = cur.left
        cur = st.pop()
        k -= 1
        if k == 0: return cur.val
        cur = cur.right
    raise ValueError("k is out of range")
# O(h + k) часу, O(h) пам’яті.
